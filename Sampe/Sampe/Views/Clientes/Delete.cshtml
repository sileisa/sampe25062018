@model Sampe.Cliente
@{
    ViewBag.Title = "Deletar";
}
<div class="container ">
    <div class="col-xs-10 col-sm-10 col-md-6">
        <ol class="breadcrumb">
            <li><a href="http://localhost:49630/Home/Index">Home</a></li>
            <li><a href="http://localhost:49630/Home/Cadastros">Cadastros</a></li>
            <li><a href="http://localhost:49630/Clientes/Index">Clientes</a></li>
            <li class="active">Deletar</li>
        </ol>
        <br />
        <h2>Deletar Cliente</h2>
        <h6>Tem certeza que deseja excluir?</h6>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Cliente
            </dt>
            <dd>
                @Html.DisplayFor(model => model.NomeCliente)
            </dd>
            <dt>
                CNPJ
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Cnpj)
            </dd>
        </dl>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Deletar" class="btn btn-default" />
                <a class="btn btn-default" href="/Clientes/Index">Voltar</a>
            </div>
        }
    </div>
</div>
</div>