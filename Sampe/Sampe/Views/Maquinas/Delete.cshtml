@model Sampe.Maquina

@{
    ViewBag.Title = "Deletar Máquina";
}

<div class="container ">
	<div class="col-xs-10 col-sm-10 col-md-6">
		<ol class="breadcrumb">
			<li><a href="/Home/Index">Home</a></li>
			<li><a href="/Home/Cadastros">Cadastros</a></li>
			<li><a href="/Maquinas/Index">Máquinas</a></li>
			<li class="active">Deletar</li>
		</ol>
		<h2>Deletar Máquina</h2>
		<h6>Tem certeza que deseja excluir?</h6>
		<hr />
		<dl>
			<dt>
				Máquina
			</dt>
			<dd>
				@Html.DisplayFor(model => model.NomeMaquina)
			</dd>
		</dl>
		<p style="color:red;">@ViewBag.Error</p>
		@using (Html.BeginForm())
	{
		@Html.AntiForgeryToken()

		<div class="form-actions no-color">
			<input type="submit" value="Deletar" class="btn btn-default" />
			<a class="btn btn-default " href="/Maquinas/Index">Voltar</a>
		</div>
}
	</div>
</div>
