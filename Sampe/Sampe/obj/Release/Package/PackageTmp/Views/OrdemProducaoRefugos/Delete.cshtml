@model Sampe.Models.OrdemProducaoRefugo
@{
	ViewBag.Title = "Delete";
}
<ol class="breadcrumb">
	<li><a href="/Home/Index">Home</a></li>
	<li><a href="/OrdemProducaoRefugos/Index">Ordens de Produção de Refugo</a></li>
	<li class="active">Editar</li>
</ol>
<h2>Deletar Ordem de Serviço  @Model.OrdemProducaoRefugoId</h2>
<h4>Tem certeza que deseja deletar?</h4>
<hr />
<div>
	
	<dl class="dl-horizontal">
		<dt>
			@Html.DisplayNameFor(model => model.Produto)
		</dt>
		<dd>
			@Html.DisplayFor(model => model.Produto)
		</dd>

		<dt>
			@Html.DisplayNameFor(model => model.Data)
		</dt>
		<dd>
			@Model.Data.ToString("dd/MM/yyyy")
		</dd>
		<dt>
			Produção Início
		</dt>
		<dd>
			@Html.DisplayFor(model => model.ProdIncio)
		</dd>
		<dt>
			Produção Fim
		</dt>
		<dd>
			@Html.DisplayFor(model => model.ProdFim)
		</dd>
		<dt>
			Operador
		</dt>
		<dd>
			@Html.DisplayFor(model => model.Usuario.NomeUsuario)
		</dd>
	</dl>
	<br />
	
	<br />
	@using (Html.BeginForm())
	{
		@Html.AntiForgeryToken()

		<div class="form-actions no-color">
			<input type="submit" value="Deletar" class="btn btn-default" />
			<a class="btn btn-default" href="/OrdemProducaoRefugos/Index">Voltar</a>
		</div>
	}
</div>
