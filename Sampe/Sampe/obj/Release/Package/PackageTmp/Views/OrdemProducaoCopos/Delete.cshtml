@model Sampe.Models.OrdemProducaoCopo

@{
	ViewBag.Title = "Delete";
}

<ol class="breadcrumb">
	<li><a href="/Home/Index">Home</a></li>
	<li><a href="/OrdemProducaoCopos/Index">Ordens de Produção de Copos</a></li>
	<li class="active">Deletar</li>
</ol>
<h2>Deletar Ordem de Serviço  @Model.CodigoIdentificador</h2>
<h4>Tem certeza que deseja deletar?</h4>
<hr />
<div>

	<dl class="dl-horizontal">
		<dt>
			@Html.DisplayNameFor(model => model.Expectativa.Produto)
		</dt>

		<dd>
			@Html.DisplayFor(model => model.Expectativa.Produto)
		</dd>
		<dt>
			Total Produzido
		</dt>

		<dd>
			@Html.DisplayFor(model => model.TotalProduzidos)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.Data)
		</dt>

		<dd>
			@Model.Data.ToString("dd/MM/yyyy")
		</dd>

		<dt>
			Produção Início
		</dt>

		<dd>
			@Html.DisplayFor(model => model.ProdIncio)
		</dd>

		<dt>
			Produção Fim
		</dt>

		<dd>
			@Html.DisplayFor(model => model.ProdFim)
		</dd>
	</dl>
	<br />
	
	<br />
	@using (Html.BeginForm())
	{
		@Html.AntiForgeryToken()

		<div class="form-actions no-color">
			<input type="submit" value="Deletar" class="btn btn-default" />
			<a class="btn btn-default" href="/OrdemProducaoCopos/Index">Voltar</a>
		</div>
	}
</div>
