@model Sampe.Models.Expectativa
@{
    ViewBag.Title = "Deletar";
}
<div class="container ">
    <div class="col-xs-10 col-sm-10 col-md-10">
		<ol class="breadcrumb">
			<li><a href="/Home/Index">Home</a></li>
			<li><a href="/Home/Cadastros">Cadastros</a></li>
			<li><a href="/Expectativas/Index">Expectativas</a></li>
			<li class="active">Nova Expectativa</li>
		</ol>
        <h2>Deletar Expectativa</h2>
        <h3>Realmente deseja deletar?</h3>
        <div>
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Produto)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Produto)
                </dd>
                <dt>
                    Cavidade do Molde
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.CavidadeMolde)
                </dd>
                <dt>
                    Peso Aproximado Peça
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.PesoPecaAproximado)
                </dd>
                <dt>
                    Peso da Peça Completa
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.PesoPecaCompleta)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Ciclo)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Ciclo)
                </dd>
                <dt>
                    Produção Esperada
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.ProducaoEsperada)
                </dd>
               
            </dl>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Deletar" class="btn btn-default" /> |
                    <a class="btn btn-default" href="/Expectativas/Index">Voltar</a>
                </div>
            }
        </div>
    </div>
</div>
