@model Sampe.Molde
@{
	ViewBag.Title = "Deletar Molde";
}
<div class="container ">
	<div class="col-xs-10 col-sm-10 col-md-6">
		<ol class="breadcrumb">
			<li><a href="/Home/Index">Home</a></li>
			<li><a href="/Home/Cadastros">Cadastros</a></li>
			<li><a href="/Moldes/Index">Moldes</a></li>
			<li class="active">Deletar</li>
		</ol>
		<h2>Deletar Molde</h2>
		<h6>Tem certeza que deseja excluir?</h6>
		<div class="row">
			<hr />
			<dl>
				<dt>
					Molde
				</dt>
				<dd>
					@Html.DisplayFor(model => model.NomeMolde)
				</dd>
				<dt>
					@Html.DisplayNameFor(model => model.Cavidade)
				</dt>
				<dd>
					@Html.DisplayFor(model => model.Cavidade)
				</dd>
			</dl>
			<p style="color:red;">@ViewBag.Error</p>
			@using (Html.BeginForm())
	{
		@Html.AntiForgeryToken()

		<div class="form-actions no-color">
			<input type="submit" value="Deletar" class="btn btn-default" />
			<a class="btn btn-default" href="/Moldes/Index">Voltar</a>
		</div>
}
		</div>
	</div>
</div>
